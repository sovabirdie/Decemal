#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include "s21_math.h"

#test abs_1
    int x = 0;
    ck_assert_int_eq(s21_abs(x), abs(x));

#test abs_2
    int x = -0;
    ck_assert_int_eq(s21_abs(x), abs(x));

#test abs_3
    int x = -10;
    ck_assert_int_eq(s21_abs(x), abs(x));

#test abs_4
    int x = 100;
    ck_assert_int_eq(s21_abs(x), abs(x));

#test abs_5
    int x = S21_INT_MIN;
    ck_assert_int_eq(s21_abs(x), abs(x));

#test abs_6
    int x = S21_INT_MAX;
    ck_assert_int_eq(s21_abs(x), abs(x));    

#test abs_7
    int x=S21_NAN;
    ck_assert_int_eq(s21_abs(x), abs(x));

#test abs_8
    int x=S21_INF;
    ck_assert_int_eq(s21_abs(x), abs(x));

