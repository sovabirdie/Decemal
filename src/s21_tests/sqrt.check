#include "s21_math.h"
#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <float.h>

#test sqrt_0
    double x = -4.25;
    ck_assert_double_nan(s21_sqrt(x));

#test sqrt_1
    double x = 1.0;
    ck_assert_ldouble_eq_tol(s21_sqrt(x), sqrtl(x), S21_EPS_TEST);

#test sqrt_2
    double x = 6.25;
    ck_assert_ldouble_eq_tol(s21_sqrt(x), sqrtl(x), S21_EPS_TEST);

