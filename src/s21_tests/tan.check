#include "s21_math.h"
#include <math.h>
#include <stdio.h>
#include <stdlib.h>

#test tan_0
    double n = -6.283185307179586;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_1
    double n = -5.497787143782138;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_2
    double n = -S21_PI / 2 - S21_PI / 40;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_3
    double n = -3.9269908169872414;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_4
    double n = -3.141592653589793;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_5
    double n = -2.356194490192345;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_6
    double n = S21_PI / 2 + S21_PI / 40;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_7
    double n = -0.7853981633974483;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_8
    double n = 0.0;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);

#test tan_9
    double n = 0.7853981633974483;
    ck_assert_ldouble_eq_tol(s21_tan(n), tanl(n), S21_EPS_TEST);


