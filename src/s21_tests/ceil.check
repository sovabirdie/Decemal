#include "s21_math.h"
#include <math.h>
#include <stdio.h>
#include <stdlib.h>

#test ceil_1
    double x=0.0;
    ck_assert_ldouble_eq(s21_ceil(0), ceill(0));

#test ceil_2
    double x=1.0;
    ck_assert_ldouble_eq(s21_ceil(x), ceill(x));

#test ceil_3
    double x=S21_INF;
    ck_assert_double_infinite(s21_ceil(x));
//    ck_assert_ldouble_eq_tol(s21_ceil(x), ceill(x), S21_EPS_TEST);

#test ceil_4
    double x=S21_NAN;
    ck_assert_ldouble_nan(s21_ceil(x));
    // ck_assert_ldouble_eq_tol(s21_ceil(x), ceill(x), S21_EPS_TEST);

#test ceil_5
    double x;
    for (x = 1; x < 100; x *= (x+1)) {
    ck_assert_ldouble_eq(s21_ceil(x), ceill(x));
    }

#test ceil_6
    double x;
    for (x = -1; x > -1e+15; x *=M_PI) {
//        ck_assert_ldouble_eq_tol(s21_ceil(x), ceil(x), S21_EPS_TEST);
        ck_assert_ldouble_eq(s21_ceil(x), ceill(x));

    }

#test ceil_7
    double x;
    for (x = 1; x > S21_EPS_TEST; x /=(0.5*M_PI)) {
        ck_assert_ldouble_eq_tol(s21_ceil(x), ceill(x), S21_EPS_TEST);
    }

#test ceil_8
    double x=-0;
    ck_assert_ldouble_eq_tol(s21_ceil(x), ceill(x), S21_EPS_TEST);

#test ceil_9
    double x;
    for (x = 4.0000000004; x > 3.9999042672; x -=0.000000011) {
        ck_assert_ldouble_eq_tol(s21_ceil(x), ceill(x), S21_EPS_TEST);
    }

#test ceil_10
    double x;
    for (x = -4.0000000004; x > -3.9999042672; x +=0.000000011) {
        ck_assert_ldouble_eq_tol(s21_ceil(x), ceill(x), S21_EPS_TEST);
    }