#include "s21_math.h"
#include <math.h>
#include <stdio.h>
#include <stdlib.h>

#test acos_1
    double x=0;
    ck_assert_ldouble_eq_tol(s21_acos(x), acosl(x), S21_EPS_TEST);

#test acos_2
    double x=1;
    ck_assert_ldouble_eq_tol(s21_acos(x), acosl(x), S21_EPS_TEST);

#test acos_3
    double x=S21_INF;
    ck_assert_ldouble_nan(s21_acos(x));
//    ck_assert_ldouble_eq_tol(s21_acos(x), acosl(x), S21_EPS_TEST);

#test acos_4
    double x=S21_NAN;
    ck_assert_ldouble_nan(s21_acos(x));
    // ck_assert_ldouble_eq_tol(s21_acos(x), acosl(x), S21_EPS_TEST);

#test acos_5
    double x;
    char s21_res[10];
    char res[10];
    for (x = 1; x < 100; x *= (x+1)) {
        sprintf(s21_res, "%.6Lf", s21_acos(x));
        sprintf(res, "%.6Lf", acosl(x));
        ck_assert_pstr_eq(s21_res, res);
    }

#test acos_6
    double x;
    char s21_res[10];
    char res[10];
    for (x = -1; x > -1e+100; x *=M_PI) {
        sprintf(s21_res, "%.6Lf", s21_acos(x));
        sprintf(res, "%.6Lf", acosl(x));
        ck_assert_pstr_eq(s21_res, res);
    }

#test acos_7
    double x;
    for (x = 1; x > S21_EPS_TEST; x /=(0.5*M_PI)) {
        ck_assert_ldouble_eq_tol(s21_acos(x), acosl(x), S21_EPS_TEST);
    }

#test acos_8
    double x;
    for (x = -1; x < 0; x +=0.001) {
        ck_assert_ldouble_eq_tol(s21_acos(x), acosl(x), S21_EPS_TEST);
        }